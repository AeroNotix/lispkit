<html>
  <body>
    <h3>LispKit cheatsheet</h3>
    <table border=1>
      <tr align=center>
        <td>Keybind</td>
        <td>Command</td>
        <td>Function</td>
      </tr>
      {% for keymap in keymaps %}
        {% for entry in keymap %}
          <tr>
            <td>{{ entry.key }}</td>
            <td>{{entry.name}}</td>
            <td>{{ entry.command }}</td>
          </tr>
        {% endfor %}
      {% endfor %}
  </body>
</html>
