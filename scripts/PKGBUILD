pkgname=lispkit-browser
pkgver=1
pkgrel=1
epoch=
pkgdesc="LispKit: A lispy, keyboard-driven browser written in Common Lisp."
arch=('x86_64')
url="https://github.com/AeroNotix/lispkit"
license=('BSD')
provides=('lispkit-browser')
source=('http://zerolength.com/bin/lispkit')
